@article{Li:2017ad,
	Author = {Li, Taiwen and Fan, Jingyu and Wang, Binbin and Traugh, Nicole and Chen, Qianming and Liu, Jun S. and Li, Bo and Liu, X. Shirley},
	Date = {2017/11/01},
	Date-Added = {2018-09-19 16:44:28 -0700},
