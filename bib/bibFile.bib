	La = {eng},
	Month = {09},
	Number = {9},
	Pages = {3238--3245},
	Publisher = {American Society for Microbiology},
	Title = {Nucleic acid (DNA, RNA) quantification and RNA/DNA ratio determination in marine sediments: comparison of spectrophotometric, fluorometric, and HighPerformance liquid chromatography methods and estimation of detrital DNA},
	Ty = {JOUR},
