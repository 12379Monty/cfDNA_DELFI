	Month = {8/22/2020},
	Number = {1},
	Pages = {63--70},
	Title = {How many samples are needed to build a classifier: a general sequential approach},
	Ty = {JOUR},
	Url = {https://doi.org/10.1093/bioinformatics/bth461},
	Volume = {21},
