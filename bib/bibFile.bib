	J1 = {J Carcinog Mutagen},
	Journal = {Journal of carcinogenesis \& mutagenesis},
	Pages = {1000198},
	Title = {Differential Effect of Active Smoking on Gene Expression in Male and Female Smokers},
	Ty = {JOUR},
